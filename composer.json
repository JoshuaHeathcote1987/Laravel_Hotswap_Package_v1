{
    "name": "joshlogic/hotswap",
    "description": "A Laravel package that enables modular development by generating self-contained packages with routes, migrations, service providers, and frontend scaffolding.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Heathcote",
            "email": "joshuaheathcote1987@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "module",
        "package",
        "scaffolding",
        "inertia",
        "vite",
        "modular"
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "JoshLogic\\Hotswap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoshLogic\\Hotswap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoshLogic\\Hotswap\\HotswapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}